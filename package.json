{
  "name": "skilltimebank",
  "version": "0.0.0",
  "description": "A community timebank app for Reddit",
  "main": "src/main.tsx",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev:run": "devvit playtest r/SkillTimeBank",
    "dev:upload": "devvit upload",
    "test": "vitest",
    "admin:award-credit": "npx tsx src/scripts/award-credit.ts",
    "admin:reset-economy": "npx tsx src/scripts/reset-economy.ts",
    "admin:ban-user": "npx tsx src/scripts/ban-user.ts"
  },
  "keywords": [
    "devvit",
    "reddit",
    "timebank"
  ],
  "author": "",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@devvit/kit": "^1.1.4",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "jsdom": "^27.0.1",
    "tsx": "^4.0.0",
    "typescript": "5.8.3",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@devvit/public-api": "0.12.1",
    "@tailwindcss/forms": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0"
  },
  "type": "module",
  "private": true
}